{
  "name": "carbon-react-app",
  "description": "Set up a React with Carbon Components by running one command.",
  "repository": {},
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "start": "webpack-dev-server --env.mode development --open --host 0.0.0.0",
    "dev": "yarn run webpack-dev-server --env.mode development --hot",
    "dev:typescript": "yarn run dev -- --env.presets typescript",
    "build": "yarn run webpack --env.mode production",
    "build:analyze": "yarn run build --env.presets analyze",
    "build:compress": "yarn run build --env.presets compress",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js|jsx}\" --quiet",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@carbon/grid": "10.10.2",
    "@carbon/icons-react": "10.10.2",
    "carbon-components": "^10.9.0",
    "carbon-components-react": "^7.9.0",
    "carbon-icons": "^7.0.7",
    "core-js": "2.5.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "3.5.3",
    "dotenv-webpack": "^1.7.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "4.2.0",
    "jest": "25.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "7.0.0",
    "prettier": "2.0.5",
    "react-loadable": "^5.5.0",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.0",
    "stylelint": "13.3.3",
    "stylelint-config-property-sort-order-smacss": "6.0.0",
    "stylelint-config-sass-guidelines": "7.0.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
